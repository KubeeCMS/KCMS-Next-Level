{
  "name": "kubeecms/kcms-next-level",
  "type": "wordpress-plugin",
  "license": "GNU",
  "description":
    "Take the user experience to the next level with future of mobile web...",
  "homepage": "https://github.com/KubeeCMS/KCMS-Next-Level/",
  "keywords": ["kubeecms", "kubee", "kcms-drive"],
  "support": {
    "issues": "https://github.com/KubeeCMS/KCMS-Next-Level/issues/",
    "forum": "https://discourse.kubee.com/"
  },
  "autoload": {
    "psr-4": {
      "Kubeecms\\Kcms\\": "/"
    }
  },
  "require": {
    "php": ">=7.3",
    "composer/installers": "^1.10"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.5"
  },
  "scripts": {
    "test": ["phpcs --ignore=vendor --extensions=php --standard=PSR2 ."]
  }
}
